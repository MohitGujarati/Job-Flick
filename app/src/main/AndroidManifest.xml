<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Linkedin_Clone"
        tools:targetApi="31">
        <activity
            android:name=".uiElements.Uic_UserProfile.UserProfileHeader"
            android:exported="false"
            android:label="@string/title_activity_user_profile_header"
            android:theme="@style/Theme.Linkedin_Clone" />
        <activity
            android:name=".uiElements.user_LoginIn"
            android:exported="false" />
        <activity
            android:name=".user_ApplyInforActivity"
            android:exported="false" />
        <activity
            android:name=".uiElements.user_Topbar"
            android:exported="false" />
        <activity
            android:name=".uiElements.user_HomeActivity"
            android:exported="true"
            android:label="@string/title_activity_user_home"
            android:theme="@style/Theme.Linkedin_Clone">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".uiElements.userHomePage"
            android:exported="false"
            android:label="@string/title_activity_user_home_page"
            android:theme="@style/Theme.Linkedin_Clone" />
        <activity
            android:name=".uiElements.user_FeatureSection"
            android:exported="false"
            android:label="@string/title_activity_user_feature_section"
            android:theme="@style/Theme.Linkedin_Clone" />
        <activity
            android:name=".uiElements.highlightsSection"
            android:exported="false" />
        <activity
            android:name=".uiElements.user_informationSection"
            android:exported="false" />
        <activity
            android:name=".uiElements.userProfileSection"
            android:exported="false" />
        <activity
            android:name=".uiElements.userProfile"
            android:exported="false"
            android:theme="@style/Theme.Linkedin_Clone" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>